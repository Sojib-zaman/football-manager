<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: cyan; -fx-border-color: red; -fx-border-radius: 4pc;" stylesheets="@transfer.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Network.Transferhome">
   <children>
      <Label id="translebel" fx:id="translabel" alignment="CENTER" depthTest="ENABLE" layoutX="104.0" layoutY="14.0" prefHeight="98.0" prefWidth="409.0" text="TRANSFER WINDOW" textFill="#36dc14">
         <font>
            <Font name="Constantia Bold" size="38.0" />
         </font>
      </Label>
      <Button fx:id="buybutton" layoutX="111.0" layoutY="155.0" mnemonicParsing="false" onAction="#BUYPLAYERclicked" prefHeight="70.0" prefWidth="207.0" style="-fx-alignment: center; -fx-background-color: lime; -fx-border-color: black;" text="Buy Player">
         <font>
            <Font name="Constantia" size="29.0" />
         </font>
      </Button>
      <Button fx:id="sellbutton" layoutX="314.0" layoutY="155.0" mnemonicParsing="false" onAction="#SELLPLAYERclicked" prefHeight="70.0" prefWidth="207.0" style="-fx-alignment: center; -fx-background-color: lime; -fx-border-color: black;" text="Sell Player">
         <font>
            <Font name="Constantia" size="29.0" />
         </font>
      </Button>
      <Button layoutX="213.0" layoutY="321.0" mnemonicParsing="false" onAction="#backtohome" prefHeight="49.0" prefWidth="207.0" style="-fx-background-color: #00b300; -fx-border-color: BLACK; -fx-border-width: 1.4PX; -fx-cursor: HELP;" text="BACK TO HOME" textFill="#560b0b">
         <font>
            <Font name="Constantia" size="22.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
